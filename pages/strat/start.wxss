/* pages/strat/start.wxss */
.container {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  height: 100%;
}

.logo {
  width: 100%;
  height: 100%;
  object-fit: cover; /* 让图片按比例填充整个容器 */
  position: absolute; /* 使用绝对定位 */
  top: 0; /* 位于页面顶部 */
  left: 0; /* 位于页面左侧 */
}


